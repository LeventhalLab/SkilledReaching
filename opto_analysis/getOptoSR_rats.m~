function opto_SR_rats = getOptoSR_rats()

ratIDs = [118,122,123,127,128,129,130,132,135,136,143:152];

startDates = {'06-27-2016',...      % 118
              '07-18-2016',...      % 122
              '06-27-2016',...      % 123
              '08-08-2016',...      % 127
              '08-15-2016',...      % 128
              '07-25-2016',...      % 129
              '07-25-2016',...      % 130
              '08-01-2016',...      % 132
              '07-25-2016',...      % 135
              '07-25-2016',...      % 136
              '10-12-2016',...      };
          
endDates = {'08-26-2016',...
            '09-16-2016',...
            '08-26-2016',...
            '10-07-2016',...
            '10-14-2016',...
            '09-24-2016',...
            '09-24-2016',...
            '09-30-2016',...
            '09-24-2016',...
            '09-24-2016'};

for ii = 1 : length(ratIDs)
    
    opto_SR_rats(ii).ratID = ratIDs(ii);
    if ii <= length(startDates)
        opto_SR_rats(ii).startDate = startDates{ii};
        opto_SR_rats(ii).endDate = endDates{ii};
    end
    
end
              