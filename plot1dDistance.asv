function h=plot1dDistance(allAlignedXyzDistPawCenters)
    h = figure;
    xlabel('x');
    ylabel('y');
    zlabel('z');
    grid on; 
    box on;
    hold on;
    
    for i=1:numel(allAlignedXyzDistPawCenters)
        colormapline(1:numel(alignedXyzDistPawCenters),smoothn((alignedXyzDistPawCenters),10,'robust'),[]);
    end
end